#Assigning elements to list
list1=[]
list1.append(1)
list1.append(2)
list1.append(3)
list1.append(4)
list2=[]
list2.append(10)
list2.append(20)
list2.append(40)
print("list1:",list1)
print("list2:",list2)

#Accessing elements from tuples 
mytuple=("C","C++","Python","java","Spark")
print(mytuple)
print(mytuple[2])

#Deleting different dictionary elements 
mydict={1:10,2:20,3:30,4:40}
print(mydict)
mydict.pop(4)
print(mydict)

